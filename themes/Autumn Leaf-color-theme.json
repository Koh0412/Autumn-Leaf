{
  "name": "Autumn Leaf",
	"colors": {
    "activityBar.background": "#3b3f36",
    "activityBar.activeBorder": "#74b103",
    "activityBar.foreground": "#aec08d",
    "activityBarBadge.background": "#679e00",
    "badge.background": "#596444",
		"editor.background": "#1F211D",
		"editorCursor.foreground": "#F8F8F0",
		"editor.foreground": "#c8d1bd",
		"editorWhitespace.foreground": "#3B3A32",
		"editor.lineHighlightBackground": "#3E3D32",
    "editor.selectionBackground": "#49483E",
    "editorLineNumber.foreground": "#606951",
    "editorGroupHeader.tabsBackground": "#262923",
    "focusBorder": "#69911f",
    "foreground": "#c8d1bd",
    "input.border": "#525555",
    "list.hoverBackground": "#3c4138b9",
    "list.focusBackground": "#3c4138b9",
    "list.activeSelectionBackground": "#535a4db9",
    "list.inactiveSelectionBackground": "#3c4138b9",
    "menu.background": "#191b18",
    "notifications.background": "#282b25",
    "panel.background": "#1F211D",
    "progressBar.background": "#679e00",
    "sideBar.background": "#282b25",
    "sideBarSectionHeader.background": "#373b33",
    "statusBar.background": "#3b442a",
		"statusBarItem.remoteBackground": "#679e00",
		"statusBar.debuggingBackground": "#3b442a",
		"statusBar.noFolderBackground": "#3b442a",
    "statusBar.foreground": "#e4e6e0",
    "titleBar.activeBackground": "#323530",
    "titleBar.activeForeground": "#c8d1bd",
    "tab.activeForeground": "#dce6d1",
    "tab.activeBackground": "#373b33",
    "tab.inactiveBackground": "#262923",
    "tab.border": "#181a16",
    "terminal.ansiBrightYellow": "#e7db36",
    "terminal.ansiBlue": "#1f76e9",
    "terminal.ansiBrightRed": "#f33a22",
    "terminal.ansiBrightMagenta": "#ee3059",
    "terminal.ansiBrightBlack": "#6e787e",
	},
	"tokenColors": [
		{
      "name": "Comment",
      "scope": [
        "comment",
        "string.comment"
      ],
      "settings": {
				"foreground": "#75715E",
				"fontStyle": "italic"
			},
		},
		{
      "name": "String",
      "scope": [
        "string",
        "string.template"
      ],
      "settings": {
        "foreground": "#FFCB6B"
      }
		},
		{
      "name": "Number",
      "scope": "constant.numeric",
      "settings": {
        "foreground": "#cc7fff"
      }
    },
    {
      "name": "Embedded String Begin/End",
      "scope": [
        "string.embedded.begin",
        "string.embedded.end",
        "punctuation.definition.template-expression",
        "punctuation.section.embedded"
      ],
      "settings": {
        "foreground": "#cc7fff"
      }
    },
    {
      "name": "Default Embedded Color",
      "scope": [
        "punctuation.section.embedded.begin.js",
        "punctuation.section.embedded.end.js",
        "punctuation.section.embedded.begin.erb",
        "punctuation.section.embedded.end.erb",
        "source.elixir.embedded",
        "punctuation.separator",
        "punctuation.accessor",
        "meta.brace"
      ],
      "settings": {
        "foreground": "#c8d1bd"
      }
    },
		{
      "name": "Built-in constant",
      "scope": "constant.language",
      "settings": {
        "foreground": "#EB917B"
      }
    },
    {
      "name": "User-defined constant",
      "scope": [
        "constant.character",
        "constant.other"
      ],
      "settings": {
        "foreground": "#cc7fff"
      }
    },
    {
      "name": "Language Variable",
      "scope": "variable.language",
      "settings": {
        "foreground": "#E64565"
      }
    },
    {
      "name": "Other variable",
      "scope": [
        "variable.other",
        "variable.other.property",
        "variable.other.block"
      ],
      "settings": {
        "foreground": "#c8d1bd"
      }
    },
    {
      "name": "Keyword",
      "scope": [
        "keyword",
        "keyword.operator.logical",
        "keyword.operator.constructor"
      ],
      "settings": {
        "foreground": "#F2644E"
      }
    },
    {
      "name": "Keyword Operator",
      "scope": "keyword.operator",
      "settings": {
        "foreground": "#F2644E"
      }
    },
    {
      "name": "Storage",
      "scope": "storage",
      "settings": {
        "foreground": "#E64565"
      }
    },
		{
      "name": "Types",
      "scope": "storage.type",
      "settings": {
        "foreground": "#cc7fff"
      }
    },
    {
      "name": "Class",
      "scope": [
        "entity.name.class",
        "entity.name.module",
        "entity.name.type",
        "storage.identifier",
        "support.class"
      ],
      "settings": {
        "foreground": "#2DE069",
      }
    },
    {
      "name": "Variable Object",
      "scope": [
        "variable.other.object",
        "variable.other.constant",
        "variable.other.global",
        "variable.other.readwrite.class",
        "variable.other.readwrite.instance",
        "variable.other.readwrite.batchfile",
        "variable.readwrite",
        "variable.readwrite.other.block"
      ],
      "settings": {
        "foreground": "#c8d1bd"
      }
    },
    {
      "name": "Inherited Class",
      "scope": "entity.other.inherited-class",
      "settings": {
        "foreground": "#2DE069",
      }
    },
    {
      "name": "Function name",
      "scope": [
        "entity.name.function",
        "support.function"
      ],
      "settings": {
        "foreground": "#A5DE50"
      }
    },
    {
      "name": "Function argument",
      "scope": [
        "variable.parameter",
        "entity.name.variable.parameter",
        "parameter.variable"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#fdaf1f"
      }
    },
    {
      "name": "Function call",
      "scope": "entity.name.function-call",
      "settings": {
        "foreground": "#A5DE50"
      }
    },
    {
      "name": "Builtin Functions",
      "scope": [
        "function.support.builtin",
        "function.support.core"
      ],
      "settings": {
        "foreground": "#A5DE50"
      }
    },
    {
      "name": "Tag Name",
      "scope": [
        "entity.name.tag",
        "entity.name.tag.class.js"
      ],
      "settings": {
        "foreground": "#E66B53"
      }
    },
    {
      "name": "Tag Attribute",
      "scope": "entity.other.attribute-name",
      "settings": {
        "foreground": "#B6DE3C"
      }
    },
    {
      "name": "Library constant",
      "scope": "support.constant",
      "settings": {
        "foreground": "#56d8c7"
      }
    },
    {
      "name": "Library function",
      "scope": "support.function",
      "settings": {
        "foreground": "#B6DE3C"
      }
    },
    {
      "name": "Library class/type",
      "scope": [
        "support.type",
        "support.variable"
      ],
      "settings": {
        "foreground": "#2DE069"
      }
    },
    {
      "name": "Json Property",
      "scope": "support.dictionary.json",
      "settings": {
        "foreground": "#2DE069"
      }
    },
    {
      "name": "Library variable",
      "scope": "support.other.variable",
      "settings": {
        "fontStyle": ""
      }
    },
    {
      "name": "StyleSheet Variable",
      "scope": [
        "variable.css",
        "variable.scss",
        "variable.less",
        "variable.sass",
      ],
      "settings": {
        "foreground": "#56d8c7"
      }
    },
    {
      "name": "Invalid",
      "scope": "invalid",
      "settings": {
        "foreground": "#F8F8F0"
      }
    },
    {
      "name": "Invalid deprecated",
      "scope": "invalid.deprecated",
      "settings": {
        "foreground": "#F8F8F0"
      }
    },
    {
      "name": "Markup: Emphasis",
      "scope": "markup.italic, markup.italic.markdown",
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "name": "Markup: Punctuation",
      "scope": [
        "punctuation.definition.italic.markdown",
        "punctuation.definition.bold.markdown",
        "punctuation.definition.heading.markdown"
      ],
      "settings": {
        "foreground": "#676f7d"
      }
    },
    {
      "name": "Markup: Emphasis Punctuation",
      "scope": "punctuation.definition.italic.markdown",
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "name": "Markdown: Link",
      "scope": "markup.underline.link.markdown",
      "settings": {
        "foreground": "#56d8c7"
      }
    },
    {
      "name": "Markup: Bold",
      "scope": "markup.bold.markdown",
      "settings": {
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markup: Heading",
      "scope": "markup.heading.markdown",
      "settings": {
        "fontStyle": "italic bold",
        "foreground": "#F2644E"
      }
    },
    {
      "name": "Markup: Quote",
      "scope": "markup.quote.markdown",
      "settings": {
        "foreground": "#98c379"
      }
    },
    {
      "name": "Markup: Separator",
      "scope": "meta.separator.markdown",
      "settings": {
        "foreground": "#c678dd",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markup: Raw",
      "scope": [
        "markup.raw.inline.markdown",
        "markup.raw.block.markdown"
      ],
      "settings": {
        "foreground": "#56b6c2"
      }
    },
    {
      "name": "Markup: List Punctuation",
      "scope": "punctuation.definition.list_item.markdown",
      "settings": {
        "fontStyle": "bold",
        "foreground": "#ffffff"
      }
    }
	],
}